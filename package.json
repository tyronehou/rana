{
  "name": "rana",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "A TypeScript PDF viewer application",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "dev:electron": "vite --mode electron",
    "build": "tsc -b && vite build",
    "build:electron": "tsc -b && vite build --mode electron && electron-builder --linux",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "build": {
    "appId": "com.rana.app",
    "productName": "Rana",
    "directories": {
      "output": "release/${version}"
    },
    "files": [
      "dist",
      "dist-electron"
    ],
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "public/frog-icon.svg"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "public/frog-icon.svg"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "icon": "public/frog-icon.svg",
      "category": "Office",
      "mimeTypes": [
        "application/pdf"
      ]
    }
  },
  "dependencies": {
    "pdf-lib": "^1.17.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-pdf": "^9.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^7.2.4",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6"
  }
}